    <script src="{{ asset('js/vendor/bootstrap.bundle.min.js') }}"></script>
    <script src="{{ asset('js/vendor/jquery.easing.min.js') }}"></script>
    <script src="{{ asset('js/base.min.js') }}"></script>
{% endblock %}

</body>

</html>

